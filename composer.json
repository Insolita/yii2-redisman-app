{
    "name": "insolita/yii2-redisman-app",
    "description": "Yii 2 Redis Manager Application",
    "keywords": ["yii2", "redis", "php-redis", "yii2-redis"],
    "homepage": "https://yii2redis-insolita1.c9.io/",
    "type": "project",
    "license": "Mit",
    "support": {
        "source": "https://github.com/Insolita/yii2-redisman-app"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "zelenin/yii2-semantic-ui": "*",
	    "yiisoft/yii2-redis": "*",
	    "insolita/yii2-redisman": "*",
	    "lav45/yii2-aceeditor": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "repositories": [
		{
			"type": "git",
			"url": "https://github.com/Insolita/yii2-redisman.git"
		}
	],
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
